{
    "__comment": "the 'allowxxx' compiler options are intended to the INScore parser (js)",
    "compilerOptions": {
       "lib": [ "es2015", "dom" ],
       "module": "system",
        "target": "es3",
        "noImplicitAny": true,
        "outDir": "build",
        "out": "../inscoreview.js",
        "sourceMap": false,
        "allowUnreachableCode": false,
        "allowUnusedLabels": false,
        "allowJs": false
    }
}
